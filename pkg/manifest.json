{
  "name": "Submissio",
  "version": "1.0.0",
  "description": "Submit to Google forms on Steroids",
  "homepage_url": "https://mutai.dev",
  "author": "Victor Mutai",
  "manifest_version": 2,
  "minimum_chrome_version": "60",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": [
        "http://192.168.100.197:5000/*",
        "https://mutai.dev/*",
        "https://www.glassdoor.com/*"
      ],
      "js": ["content.js"]
    }
  ]
}
